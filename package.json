  "license": "MIT",
  "dependencies": {
    "commander": "^2.9.0",
    "handlebars": "^4.0.5",
    "object-assign": "^4.1.0",
    "winston": "^2.2.0",
    "xml2js": "^0.4.16"
  }
}
